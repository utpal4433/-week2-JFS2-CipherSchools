public class This_Keyword {
    int arg = 5;
    This_Keyword(){
        System.out.println("This is default constructor");
    }
    This_Keyword(int arg){
        this();
        this.arg = arg;
    }

    public static void main(String[] args) {
        int arg = 10;
        This_Keyword obj = new This_Keyword(10);
        System.out.println(obj.arg);
    }
}
